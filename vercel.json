{
    "version": 2,
    "builds": [
      {
        "src": "src/main.ts",
        "use": "@vercel/node",
        "config": {
        "includeFiles": [
          "tesseract-files/**"
        ]
      }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/main.ts"
      }
    ]
  }